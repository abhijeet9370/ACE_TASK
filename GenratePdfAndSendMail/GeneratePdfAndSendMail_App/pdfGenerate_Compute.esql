CREATE COMPUTE MODULE pdfGenerate_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		CALL CopyEntireMessage();
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		DECLARE pdfData BLOB;
		SET pdfData =  generatePdf(CAST (InputRoot.BLOB.BLOB AS CHARACTER CCSID 1208 ENCODING 546));
		SET OutputRoot.BLOB.BLOB = pdfData;
	END;
END MODULE;

CREATE PROCEDURE generatePdf (IN inData CHARACTER ) RETURNS BLOB
LANGUAGE JAVA
EXTERNAL NAME "pdfG.BlobToPdfGenerator.generatePdf";




